{
  "name": "@vendhub/api",
  "version": "1.0.0",
  "private": true,
  "description": "VendHub API - NestJS Backend",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint src test --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "npx typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d ./src/database/typeorm.config.ts",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- migration:run -d ./src/database/typeorm.config.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ./src/database/typeorm.config.ts",
    "migration:show": "npm run typeorm -- migration:show -d ./src/database/typeorm.config.ts",
    "schema:sync": "npm run typeorm -- schema:sync -d ./src/database/typeorm.config.ts",
    "schema:drop": "npm run typeorm -- schema:drop -d ./src/database/typeorm.config.ts",
    "schema:log": "npm run typeorm -- schema:log -d ./src/database/typeorm.config.ts",
    "db:seed": "ts-node -r tsconfig-paths/register ./src/database/seeds/run-seed.ts",
    "db:seed:admin": "ts-node -r tsconfig-paths/register ./src/database/seeds/admin-seed.ts",
    "db:reset": "npm run schema:drop && npm run migration:run && npm run db:seed",
    "db:migrate": "npm run migration:run"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bullmq": "^11.0.0",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.1.11",
    "@nestjs/event-emitter": "^3.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.0",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/platform-socket.io": "^11.0.0",
    "@nestjs/schedule": "^5.0.0",
    "@nestjs/swagger": "^11.0.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.0.0",
    "@sentry/node": "^8.0.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.0.0",
    "cache-manager": "^5.4.0",
    "cache-manager-redis-yet": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.10",
    "exceljs": "^4.4.0",
    "express": "^4.21.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "nanoid": "^5.0.0",
    "nestjs-cls": "^4.3.0",
    "otplib": "^12.0.1",
    "papaparse": "^5.4.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.0",
    "socket.io": "^4.7.0",
    "telegraf": "^4.16.0",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^11.0.0",
    "winston": "^3.14.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.11",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.0",
    "@types/papaparse": "^5.3.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.0"
  }
}
